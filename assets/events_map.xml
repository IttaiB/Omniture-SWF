<?xml version="1.0"?>

<omnitureEventsMap>

	<initialization>

		<reportSuites>

			<reportSuite>corus-tv-ytv</reportSuite>

		</reportSuites>

		<visitorNamespace>corus</visitorNamespace>

		<trackingServer>metrics.corus.ca</trackingServer>

		<pageName>{experience.playerName}</pageName>

		<pageURL>{experience.url}</pageURL>

		<trackClickMap value="true" />

		<currencyCode>USD</currencyCode>

		<charSet>UTF-8</charSet>

	</initialization>

	

	

	<events>

		<!-- VIDEO SPECIFIC EVENTS -->

		<event name="mediaBegin">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="begin" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:begin" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="begin" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:begin" />

			<eventNumbers value="13" />

		</event>

	

		<event name="mediaComplete">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="complete" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:complete" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="complete" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:complete" />

			<eventNumbers value="13" />

		</event>

		

		<event name="mediaPause"></event>

		<event name="mediaResume"></event>

		<event name="seekForward"></event>

		<event name="seekBackward"></event>

		<event name="mediaMuted"></event>

		<event name="mediaUnmuted"></event>

		<event name="volumeChanged"></event>

		<event name="renditionChangeRequest"></event>

		<event name="renditionChangeComplete"></event>

		

		<!-- time-based milestone example

			<event name="milestone" type="time" value="10"> 

		-->

		<event name="milestone" type="percent" value="25">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="25percent" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:25percent" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="25percent" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:25percent" />

			<eventNumbers value="13" />

		</event>

		

		<event name="milestone" type="percent" value="50">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="50percent" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:50percent" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="50percent" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:50percent" />

			<eventNumbers value="13" />

		</event>

		

		<event name="milestone" type="percent" value="75">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="75percent" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:75percent" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="75percent" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:75percent" />

			<eventNumbers value="13" />

		</event>

	

		<!-- AD SPECIFIC EVENTS -->

		<event name="adStart"></event>

		<event name="adPause"></event>

		<event name="adResume"></event>

		<event name="adComplete"></event>

		<event name="adClick"></event>

		<event name="adPostrollsComplete"></event>

		<event name="externalAd"></event>

		

		<!-- MENU/SOCIAL SPECIFIC EVENTS -->

		<event name="embedCodeRetrieved">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="embedGet" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:embedGet" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="embedGet" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:embedGet" />

			<eventNumbers value="13" />

		</event>

		<event name="linkGenerated">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="linkGet" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:linkGet" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="linkGet" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:linkGet" />

			<eventNumbers value="13" />

		</event>

		

		<event name="codeCopied">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="embedCopy" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:embedCopy" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="embedCopy" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:embedCopy" />

			<eventNumbers value="13" />

		</event>



		<event name="linkCopied">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="linkCopy" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:linkCopy" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="linkCopy" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:linkCopy" />

			<eventNumbers value="13" />

		</event>

		

		<event name="blogPosted">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="blog" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:blog" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="blog" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:blog" />

			<eventNumbers value="13" />

		</event>

		

		<event name="menuPageOpened"></event>

		

		<event name="menuPageClosed"></event>

		

		<event name="emailSent">

			<prop number="19" value="{video.customFields['AnalyticsName']}" />

			<prop number="20" value="email" />

			<prop number="21" value="{video.customFields['AnalyticsName']}:email" />

			<evar number="19" value="{video.customFields['AnalyticsName']}" />

			<evar number="20" value="email" />

			<evar number="21" value="{video.customFields['AnalyticsName']}:email" />

			<eventNumbers value="13" />

		</event>

		

		<!-- EXPERIENCE SPECIFIC EVENTS -->

		<event name="enterFullScreen"></event>

		<event name="exitFullScreen"></event>

	</events>

	

</omnitureEventsMap>